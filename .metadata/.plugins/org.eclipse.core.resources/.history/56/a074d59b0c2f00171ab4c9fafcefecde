package bit.edu;
//책 → 제목,가격,출판사,페이지수 : 상태정보
public class BookVO {
	// 1. 멤버변수(상태정보) → 보호(정보은닉)
	public String title;
	public int price;
	public String company;
	public int page;
	// 2. setter / getter method를 만들것(자동으로 만들것)

	public String toString(){
		return title+"\t"+price+"\t"+company+"\t"+page;
	}
	

}
