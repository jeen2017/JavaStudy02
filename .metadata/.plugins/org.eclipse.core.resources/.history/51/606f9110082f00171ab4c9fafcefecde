package bit.edu;
//책 → 제목,가격,출판사,페이지수 : 상태정보
public class BookVO {
	// 1. 멤버변수(상태정보) → 보호(정보은닉)
	private String title;
	private int price;
	private String company;
	private int page;
	// title에 데이터를 세팅하는 메서드 정의
	public void setTitle(String _title){  // 낙타표기법(카멜표기법)
		title=_title;  // this.title=title;
	}
	// title에 저장된 데이터를 외부로 넘겨주는 메서드를 정의
	public String getTitle(){
		return title;
	}
}
